; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="Meshiverse"
config/description="Meshiverse is a 3D model library organizer.  Designed to make managing your 3D models easier."
config/version="0.1.0"
run/main_scene="uid://ddqenfvhqshju"
config/features=PackedStringArray("4.5", "Forward Plus")
run/low_processor_mode=true
boot_splash/image="uid://dv0sito2mgf4h"
config/icon="uid://cdydkfrovytiq"
boot_splash/minimum_display_time=10

[autoload]

ConfigFileHandler="*res://singletons/ConfigFileHandler.gd"
ThemeManager="*res://singletons/theme_manager.gd"

[debug]

gdscript/warnings/untyped_declaration=2
gdscript/warnings/inferred_declaration=1

[display]

window/size/viewport_height=1024
window/size/initial_position_type=5
window/subwindows/embed_subwindows=false
window/warnings/inferred_declaration=0
window/warnings/untyped_declaration=0

[editor_plugins]

enabled=PackedStringArray("res://addons/ai_assistant_hub/plugin.cfg", "res://addons/stl-io/plugin.cfg")

[plugins]

ai_assistant_hub/llm_api="ollama_api"
ai_assistant_hub/preferences/thinking_target=0
ai_assistant_hub/preferences/skip_greeting=false
ai_assistant_hub/preferences/always_scroll_to_bottom=false

[rendering]

renderer/rendering_method="gl_compatibility"
renderer/rendering_method.mobile="gl_compatibility"
textures/vram_compression/import_etc2_astc=true
