; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="Meshiverse"
config/description="Meshiverse is a 3D model library organizer.  Designed to make managing your 3D models easier."
config/version="0.1.0"
run/main_scene="res://scenes/settings.tscn"
config/features=PackedStringArray("4.5", "Forward Plus")
run/low_processor_mode=true
config/icon="res://icon.svg"

[autoload]

ConfigFileHandler="*res://singletons/ConfigFileHandler.gd"

[debug]

gdscript/warnings/untyped_declaration=2
gdscript/warnings/inferred_declaration=1

[display]

window/size/viewport_height=1024
window/warnings/inferred_declaration=0
window/warnings/untyped_declaration=0

[rendering]

renderer/rendering_method="gl_compatibility"
renderer/rendering_method.mobile="gl_compatibility"
